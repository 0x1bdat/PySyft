{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "westus"
    },
    "networkInterfaceName": {
      "value": "GEN_UNIQUE"
    },
    "networkSecurityGroupName": {
      "value": "GEN_UNIQUE"
    },
    "networkSecurityGroupRules": {
      "value": [
        {
          "name": "SSH",
          "properties": {
            "priority": 300,
            "protocol": "TCP",
            "access": "Allow",
            "direction": "Inbound",
            "sourceAddressPrefix": "*",
            "sourcePortRange": "*",
            "destinationAddressPrefix": "*",
            "destinationPortRange": "22"
          }
        },
        {
          "name": "HTTP",
          "properties": {
            "priority": 320,
            "protocol": "TCP",
            "access": "Allow",
            "direction": "Inbound",
            "sourceAddressPrefix": "*",
            "sourcePortRange": "*",
            "destinationAddressPrefix": "*",
            "destinationPortRange": "80"
          }
        }
      ]
    },
    "subnetName": {
      "value": "default"
    },
    "virtualNetworkName": {
      "value": "GEN_UNIQUE"
    },
    "addressPrefixes": {
      "value": ["10.0.0.0/16"]
    },
    "subnets": {
      "value": [
        {
          "name": "default",
          "properties": {
            "addressPrefix": "10.0.0.0/24"
          }
        }
      ]
    },
    "publicIpAddressName": {
      "value": "domain-ip"
    },
    "publicIpAddressType": {
      "value": "Dynamic"
    },
    "publicIpAddressSku": {
      "value": "Basic"
    },
    "virtualMachineName": {
      "value": "domain"
    },
    "virtualMachineComputerName": {
      "value": "domain"
    },
    "virtualMachineRG": {
      "value": "GEN_UNIQUE"
    },
    "osDiskType": {
      "value": "StandardSSD_LRS"
    },
    "virtualMachineSize": {
      "value": "Standard_D4s_v3"
    },
    "adminUsername": {
      "value": "azureuser"
    }
  }
}
